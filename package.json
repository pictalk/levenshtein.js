{
  "name": "levenstein.js",
  "version": "1.1.0",
  "description": "Efficient implementation of Levenshtein algorithm  with locale-specific collator support.",
  "main": "lib/levenshtein",
  "types": "esm/levenshtein",
  "module": "esm/levenshtein",
  "scripts": {
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "rm -Rf lib esm && npm run build:cjs && npm run build:esm",
    "prepublishOnly": "npm run build",
    "prerelease": "npm run test",
    "release": "standard-version",
    "postrelease": "git push --follow-tags origin master && npm publish --access public",
    "test:update": "jest --verbose -u",
    "test": "jest --verbose"
  },
  "files": [
    "lib",
    "esm",
    "src"
  ],
  "keywords": [
    "levenshtein",
    "distance",
    "string"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$"
  },
  "author": "Ramesh Nair <ram@hiddentao.com> (http://www.hiddentao.com/)",
  "contributors": [
    "Alex Regan <alex.regan@pictalk.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "@types/jest": "^23.3.5",
    "@types/node": "^10.12.0",
    "benchmark": "^2.1.4",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "standard-version": "^4.4.0",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.3"
  }
}
